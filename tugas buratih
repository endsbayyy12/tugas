import React, {Component} from 'react';
import './App.css';
import Alert from './Components/Alert'

class App extends Component {

  klik = () => {
    alert("Selamat anda diterima")
  }

  render() {
    return (

      <div className='App container col-sm-6'>
          <Alert type="warning">From Pendaftaran Osis</Alert>
          <input type="text" placeholder='Nama'></input>
          <input type="text" placeholder='Jenis Kelamin'></input>
          <input type="text" placeholder='Alamat'></input>
          <button onClick={this.klik}>Submit</button>
      </div>
     
    );
  }
}
export default App;
